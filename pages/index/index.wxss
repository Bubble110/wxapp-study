.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  padding: 20px;
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .index_group .index_title {
  padding: 10rpx 0;
}
.index_floor .index_group .index_title image {
  width: 100%;
}
.index_floor .index_group .index_list {
  /*清楚浮动*/
  overflow: hidden;
}
.index_floor .index_group .index_list navigator {
  float: left;
  width: 33.33%;
  /*
        后四个超链接
        原图的高度 232*386
        232/386=33.3vw/height
        第一张图片的高度 height:33.33vw*386/232
        */
  /*2,3两个超链接*/
}
.index_floor .index_group .index_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10px solid #ffffff;
}
.index_floor .index_group .index_list navigator:nth-child(2),
.index_floor .index_group .index_list navigator:nth-child(3) {
  border-bottom: 10px solid #ffffff;
}
.index_floor .index_group .index_list navigator image {
  width: 100%;
  height: 100%;
}
